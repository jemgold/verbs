{
  "name": "verbs",
  "private": true,
  "sideEffects": [
    "polyfills.ts"
  ],
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@babel/core": ">=7.4.0 <8.0.0",
    "@rainbow-me/rainbowkit": "^0.8.1",
    "@remix-run/node": "^1.9.0",
    "@remix-run/react": "^1.9.0",
    "@remix-run/serve": "^1.9.0",
    "@remix-run/server-runtime": "^1.5.1",
    "@remix-run/vercel": "^1.9.0",
    "@tanstack/query-core": "4.20.4",
    "@tanstack/query-sync-storage-persister": "^4.20.4",
    "@tanstack/react-query": "^4.20.4",
    "@vanilla-extract/css": "^1.0.0",
    "@vanilla-extract/sprinkles": "^1.5.1",
    "@vercel/node": "^1.8.3 || ^2.4.0",
    "@zoralabs/nouns-protocol": "^1.0.1",
    "buffer-polyfill": "npm:buffer@^6.0.3",
    "cross-env": "^7.0.3",
    "ethers": "^5.7.2",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "reactflow": "^11.3.3",
    "remix-utils": "^5.1.0",
    "wagmi": "^0.9.4",
    "zod": ">=3.19.1 <4.0.0",
    "zustand": "^4.1.5"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.9.0",
    "@remix-run/eslint-config": "^1.9.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "eslint": "^8.29.0",
    "postcss": "^8.4.20",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=14"
  }
}
